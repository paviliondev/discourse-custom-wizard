<div class="admin-wizard-select admin-wizard-controls">
  {{combo-box
    value=wizardId
    content=wizardList
    onChange=(route-action "changeWizard")
    options=(hash none="admin.wizard.select")
  }}
</div>

{{wizard-message
  key=messageKey
  opts=messageOpts
  url=documentationUrl
  component="logs"
}}

<div class="admin-wizard-container">
  {{outlet}}
</div>